<h1> Create New Figure </h1>

<form action="/figures" method="post">

  <lable>Name</lable>
  <input type="text" name="figure[name]" id="figure_name">

  <lable>Titles</lable>
  <% Title.all.each do |title| %>
  <input type="radio" name="figure[title]" id="figure_title"><%= title.name %>
  <% end %>

  <lable>Landmarks</lable>




</form>
